{
  "name": "@reagentai/reagent",
  "version": "0.12.2",
  "description": "A framework to build AI agents",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "tsc:watch": "tsc --watch",
    "build": "tsc",
    "clean": "rm -rf node_modules && rm -rf dist",
    "prepublish": "rm -rf dist && pnpm build && cp package.json dist"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./rxjs": {
      "import": "./rxjs.js",
      "types": "./rxjs.d.ts"
    },
    "./chat": {
      "import": "./chat/index.js",
      "types": "./chat/index.d.ts"
    },
    "./workflow": {
      "import": "./workflow/index.js",
      "types": "./workflow/index.d.ts"
    },
    "./workflow/client": {
      "import": "./workflow/client.js",
      "types": "./workflow/client.d.ts"
    },
    "./nodes": {
      "import": "./workflow/nodes/index.js",
      "types": "./workflow/nodes/index.d.ts"
    },
    "./llm/core": {
      "import": "./llm/core/index.js",
      "types": "./llm/core/index.d.ts"
    },
    "./llm/models": {
      "import": "./llm/models/index.js",
      "types": "./llm/models/index.d.ts"
    },
    "./llm/prompt": {
      "import": "./llm/prompt/index.js",
      "types": "./llm/prompt/index.d.ts"
    },
    "./llm/executors": {
      "import": "./llm/executors/index.js",
      "types": "./llm/executors/index.d.ts"
    },
    "./llm/plugins": {
      "import": "./llm/plugins/index.js",
      "types": "./llm/plugins/index.d.ts"
    },
    "./utils": {
      "import": "./utils.js",
      "types": "./utils.d.ts"
    },
    "./vite": {
      "import": "./vite.js",
      "require": "./webpack.js",
      "types": "./vite.d.ts"
    },
    "./webpack": {
      "import": "./webpack.js",
      "require": "./webpack.js",
      "types": "./webpack.d.ts"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true
  },
  "license": "MIT",
  "keywords": [],
  "dependencies": {
    "@babel/core": "^7.24.5",
    "@babel/generator": "^7.24.5",
    "@babel/plugin-syntax-jsx": "^7.24.6",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/types": "^7.23.6",
    "@sindresorhus/slugify": "^2.2.1",
    "clean-set": "^1.1.2",
    "clsx": "^2.1.1",
    "dedent": "^1.5.1",
    "deepmerge": "^4.3.1",
    "dlv": "^1.1.3",
    "dset": "^3.1.3",
    "filter-obj": "^6.1.0",
    "immer": "^10.1.1",
    "klona": "^2.0.6",
    "ky": "^1.2.4",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.7",
    "picomatch": "^4.0.2",
    "react": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-markdown": "^9.0.1",
    "redux-saga": "^1.3.0",
    "rxjs": "^7.8.1",
    "serialize-error": "^11.0.3",
    "tiny-invariant": "^1.3.3",
    "toposort": "^2.0.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.23.0",
    "zod-validation-error": "^3.3.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.2",
    "@types/babel__core": "^7.20.5",
    "@types/dlv": "^1.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/toposort": "^2.0.7",
    "typescript": "5.4.5",
    "vitest": "^1.6.0"
  }
}
